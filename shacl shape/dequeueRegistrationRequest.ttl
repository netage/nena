@prefix nena:   <urn:oasis:shacl:shape:> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nena:DequeueRegistrationRequest
	a sh:NodeShape ;
	sh:targetClass <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:DequeueRegistrationRequest> ;
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationQueueUri> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "CancelTimeStamp" ;
		sh:description "Timestamp CANCEL was received at ESRP" ;
	] ;
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationDequeuer> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:nodeKind sh:IRI ;
		sh:name "DequeueRegistrationDequeuer" ;
		sh:description "SIP URI of dequeuer (where to send calls)" ;
	] ;
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationExpirationTime> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "InviteText" ;
		sh:description "Content of INVITE message" ;
	] ;
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationDequeuePreference> ;           
		sh:name "DequeueRegistrationDequeuePreference" ;
		sh:datatype xsd:string;
		sh:maxCount 1 ;
		sh:description "Integer from 1-5 indicating queuing preference." ;
	];
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationExpirationTime> ;           
		sh:name "DequeueRegistrationExpirationTime" ;
		sh:datatype xsd:integer;
		sh:maxCount 1 ;
		sh:description "Requested time in seconds this registration will expire.  The expirationTime in the response is the actual expiration, which may be equal to or greater than that in the request depending on the local policy of the ESRP.  A request expirationTime of zero is a request to deregister." ;
	];
	sh:property [             
		sh:path <urn:nena:xml:ns:DequeueRegistrationExchange:2.0:dequeueRegistrationQueueUri> ;           
		sh:name "DequeueRegistrationQueueUri" ;
		sh:nodeKind sh:IRI ;
		sh:maxCount 1 ;
		sh:description "SIP URI of queue to register on." ;
	].