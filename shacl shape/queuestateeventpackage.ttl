@prefix nena:   <urn:oasis:shacl:shape:> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nena:QueueStateEventPackageShape
	a sh:NodeShape ;
	sh:targetClass <http://vocab.nena.org#QueueStateEventPackage> ;
	sh:property [             
		sh:path <http://vocab.nena.org#queueStateEventUri> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "QueueStateEventUri" ;
		sh:description "SIP URI of queue" ;
		sh:datatype xsd:anyURI ;
	] ;
	sh:property [             
		sh:path <http://vocab.nena.org#queueStateEventQueueLength> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "QueueStateEventQueueLength" ;
		sh:description "Integer indicating current number of calls on the queue" ;
		sh:datatype xsd:integer ;
	] ;
	sh:property [             
		sh:path <http://vocab.nena.org#queueStateEventMaxLength> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "QueueStateEventMaxLength" ;
		sh:description "Integer indicating maximum length of queue" ;
		sh:datatype xsd:string ;
	] ;
	sh:property [             
		sh:path <http://vocab.nena.org#queueStateValuesCode> ;           
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:name "QueueStateValuesCode" ;
		sh:description "One or more entities are actively available or are currently handling calls being enqueue" ;
		sh:datatype xsd:string ;
		sh:in ("Active" "Inactive" "Disabled" "Full" "Standby");
	] .
